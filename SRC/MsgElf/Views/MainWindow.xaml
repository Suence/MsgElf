<Window x:Class="MsgElf.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MsgElf"
        xmlns:prism="http://prismlibrary.com/"
        ShowInTaskbar="False"
        xmlns:modernWpf="http://schemas.modernwpf.com/2019"
        modernWpf:WindowHelper.UseModernWindowStyle="True"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="{Binding Title}"
        Height="450"
        Width="800">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="MainRegion" />
        <ContentControl prism:RegionManager.RegionName="MessageRegion" />
    </Grid>
</Window>
